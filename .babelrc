{
    "plugins": [
        "transform-runtime"
	],
    "env": {
        "test": {
            "sourceMaps" : "inline",
            "presets": [
                ["env", {
                    "targets": {
                        "browsers": ["last 2 versions", "ie >= 9"]
                    }
                }],
                "react",
                "jest",
            ],
        },
        "development": {
            "sourceMaps" : "inline",
            "presets": [
                ["env", {
                    "targets": {
                        "browsers": ["last 2 versions", "ie >= 9"]
                    }
                }],
                "react",
            ],
        },
        "commonjs": {
            "presets": [
                ["env", {
                    "targets": {
                        "browsers": ["last 2 versions", "ie >= 9"]
                    }
                }],
                "react",
            ],
        },
        "es": {
            "presets": [
                ["env", {
                    "targets": {
                        "browsers": ["last 2 versions", "ie >= 9"]
                    },
                    "modules": false
                }],
                "react",
            ],
        },
    }
}
