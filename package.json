{
  "name": "i18n-backend",
  "version": "0.1.1",
  "description": "i18n backend for i18n server.",
  "main": "lib/src/index.js",
  "bin": "lib/src/i18n-cli.js",
  "scripts": {
    "build": "npm run clean && tsc -p . --sourceMap false",
    "build:debug": "npm run clean && tsc -p .",
    "test": "npm run build && jest",
    "cli": "npm run build && node lib/src/i18n-cli.js --url http://localhost:5000/data.txt",
    "tsc": "tsc",
    "clean": "rimraf lib",
    "prepublish": "npm run test",
    "coverage": "jest --coverage",
    "report": "nyc report --temp-directory=coverage --reporter=text-lcov | coveralls",
    "coveralls": "npm run coverage && npm run report"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wangtao0101/i18n-backend.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wangtao0101/i18n-backend/issues"
  },
  "homepage": "https://github.com/wangtao0101/i18n-backend#readme",
  "devDependencies": {
    "@types/jest": "^21.1.5",
    "@types/node": "^8.5.7",
    "axios": "^0.17.1",
    "axios-mock-adapter": "^1.10.0",
    "coveralls": "^3.0.0",
    "jest": "^21.2.1",
    "nyc": "^11.3.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^21.1.4",
    "tslint": "^5.8.0",
    "tslint-eslint-rules": "^4.1.1",
    "tslint-react": "^3.2.0",
    "typescript": "^2.6.2"
  },
  "jest": {
    "transform": {
      "^.+\\.ts?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js?)$",
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "moduleNameMapper": {
      "i18n-backend": "<rootDir>/lib/src/index.js"
    }
  },
  "dependencies": {
    "commander": "^2.12.2",
    "invariant": "^2.2.2",
    "json-format": "^1.0.1"
  }
}
